<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="eShop_BDPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.entities.SubastaUsuario</class>
    <class>com.entities.Ruta</class>
    <class>com.entities.Delivery</class>
    <class>com.entities.Ciudad</class>
    <class>com.entities.Pais</class>
    <class>com.entities.Compra</class>
    <class>com.entities.Comentario</class>
    <class>com.entities.Producto</class>
    <class>com.entities.DetalleCompra</class>
    <class>com.entities.DetalleCanje</class>
    <class>com.entities.Subasta</class>
    <class>com.entities.Usuario</class>
    <class>com.entities.DetalleEntrega</class>
    <class>com.entities.ProductoSubasta</class>
    <class>com.entities.Canje</class>
    <class>com.entities.Cartera</class>
    <class>com.entities.Puntos</class>
    <class>com.entities.Departamento</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/eshop_proyectobd?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
